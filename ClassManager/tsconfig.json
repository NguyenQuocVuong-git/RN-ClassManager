{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "src/app/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@theme": [
        "theme.js"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "*.ts",
    "*.tsx",
    "src/types/nativewind.d.ts",
    "nativewind-env.d.ts"
  ]
}
